<?xml version="1.0"?>
<rdf:RDF xmlns="https://vocab.example.org/actions/v3#"
     xml:base="https://vocab.example.org/actions/v3"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:bfo="http://purl.obolibrary.org/obo/BFO_"
     xmlns:cco="https://www.commoncoreontologies.org/"
     xmlns:schema="http://schema.org/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:dc="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://vocab.example.org/actions/v3">
        <owl:versionIRI rdf:resource="https://vocab.example.org/actions/v3/poc"/>
        <dc:creator>Darrion Burgess</dc:creator>
        <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-25</dc:created>
        <dc:title>Actions Vocabulary v3 - POC</dc:title>
        <rdfs:label>Actions Vocabulary (BFO/CCO-aligned) - Proof of Concept</rdfs:label>
        <rdfs:comment>BFO and CCO-aligned ontology for action planning and execution. This POC demonstrates the core architecture with ActionPlan (information/continuant) and ActionProcess (execution/occurrent) separation.</rdfs:comment>
        <owl:versionInfo>3.0.0-poc</owl:versionInfo>

        <!-- Import BFO and CCO -->
        <!-- NOTE: Imports commented out for Python testing. Uncomment for Protégé. -->
        <!-- <owl:imports rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"/> -->
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Core Action Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.example.org/actions/v3#ActionPlan -->

    <owl:Class rdf:about="https://vocab.example.org/actions/v3#ActionPlan">
        <rdfs:subClassOf rdf:resource="https://www.commoncoreontologies.org/ont00000965"/>
        <skos:closeMatch rdf:resource="http://schema.org/Action"/>
        <rdfs:label xml:lang="en">Action Plan</rdfs:label>
        <skos:definition xml:lang="en">A directive information content entity that prescribes an action to be performed.</skos:definition>
        <rdfs:comment xml:lang="en">ActionPlans are BFO continuants (generically dependent continuants) that exist across time and can prescribe multiple action processes. An action plan can be executed multiple times (e.g., recurring actions). Closely aligned with Schema.org Action semantics via SKOS mapping.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.example.org/actions/v3#ActionProcess -->

    <owl:Class rdf:about="https://vocab.example.org/actions/v3#ActionProcess">
        <rdfs:subClassOf rdf:resource="https://www.commoncoreontologies.org/ont00000228"/>
        <rdfs:label xml:lang="en">Action Process</rdfs:label>
        <skos:definition xml:lang="en">A planned act that is the actual execution of an action plan.</skos:definition>
        <rdfs:comment xml:lang="en">ActionProcesses are BFO occurrents (processes) that unfold over time and have temporal parts. They are prescribed by ActionPlans and represent the concrete realization of the plan's directives. Each execution of an action plan creates a distinct action process.</rdfs:comment>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Hierarchical Action Plan Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.example.org/actions/v3#RootActionPlan -->

    <owl:Class rdf:about="https://vocab.example.org/actions/v3#RootActionPlan">
        <rdfs:subClassOf rdf:resource="https://vocab.example.org/actions/v3#ActionPlan"/>
        <owl:disjointWith rdf:resource="https://vocab.example.org/actions/v3#ChildActionPlan"/>
        <owl:disjointWith rdf:resource="https://vocab.example.org/actions/v3#LeafActionPlan"/>
        <rdfs:label xml:lang="en">Root Action Plan</rdfs:label>
        <rdfs:comment xml:lang="en">A top-level action plan at hierarchical depth 0. Root action plans can belong to projects and serve as parents to child action plans. They cannot have parent actions.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.example.org/actions/v3#ChildActionPlan -->

    <owl:Class rdf:about="https://vocab.example.org/actions/v3#ChildActionPlan">
        <rdfs:subClassOf rdf:resource="https://vocab.example.org/actions/v3#ActionPlan"/>
        <owl:disjointWith rdf:resource="https://vocab.example.org/actions/v3#LeafActionPlan"/>
        <rdfs:label xml:lang="en">Child Action Plan</rdfs:label>
        <rdfs:comment xml:lang="en">An intermediate action plan at hierarchical depths 1-4. Child action plans must have a parent action and may have child or leaf action plans as parts.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.example.org/actions/v3#LeafActionPlan -->

    <owl:Class rdf:about="https://vocab.example.org/actions/v3#LeafActionPlan">
        <rdfs:subClassOf rdf:resource="https://vocab.example.org/actions/v3#ActionPlan"/>
        <rdfs:label xml:lang="en">Leaf Action Plan</rdfs:label>
        <rdfs:comment xml:lang="en">A terminal action plan at hierarchical depth 5 (maximum). Leaf action plans must have a parent child action and cannot have children of their own.</rdfs:comment>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Action State Enumeration
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.example.org/actions/v3#ActionState -->

    <owl:Class rdf:about="https://vocab.example.org/actions/v3#ActionState">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:label xml:lang="en">Action State</rdfs:label>
        <rdfs:comment xml:lang="en">An enumeration of states that an action process can be in. These are BFO qualities that inhere in action processes.</rdfs:comment>
    </owl:Class>



    <!-- State Individuals -->

    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#NotStarted">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Not Started</rdfs:label>
        <rdfs:comment xml:lang="en">Action process has not been initiated.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#InProgress">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">In Progress</rdfs:label>
        <rdfs:comment xml:lang="en">Action process is currently being executed.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#Completed">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Completed</rdfs:label>
        <rdfs:comment xml:lang="en">Action process has been successfully completed.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#Blocked">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Blocked</rdfs:label>
        <rdfs:comment xml:lang="en">Action process is blocked waiting for external dependency.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#Cancelled">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Cancelled</rdfs:label>
        <rdfs:comment xml:lang="en">Action process has been cancelled and will not be completed.</rdfs:comment>
    </owl:NamedIndividual>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.example.org/actions/v3#prescribes -->

    <owl:ObjectProperty rdf:about="https://vocab.example.org/actions/v3#prescribes">
        <rdfs:subPropertyOf rdf:resource="https://www.commoncoreontologies.org/ont00001942"/>
        <rdfs:domain rdf:resource="https://vocab.example.org/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.example.org/actions/v3#ActionProcess"/>
        <rdfs:label xml:lang="en">prescribes</rdfs:label>
        <skos:definition xml:lang="en">Relates an action plan to the action process it prescribes.</skos:definition>
        <rdfs:comment xml:lang="en">Specialized version of CCO's prescribes property (ont00001942) with domain and range restrictions. An action plan can prescribe multiple processes (e.g., recurring actions or multiple executions of the same plan).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.example.org/actions/v3#hasState -->

    <owl:ObjectProperty rdf:about="https://vocab.example.org/actions/v3#hasState">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://vocab.example.org/actions/v3#ActionProcess"/>
        <rdfs:range rdf:resource="https://vocab.example.org/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">has state</rdfs:label>
        <rdfs:comment xml:lang="en">Relates an action process to its current state. Functional property - each process has exactly one state.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/actionStatus"/>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatype Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.example.org/actions/v3#hasPriority -->

    <owl:DatatypeProperty rdf:about="https://vocab.example.org/actions/v3#hasPriority">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://vocab.example.org/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <rdfs:label xml:lang="en">has priority</rdfs:label>
        <rdfs:comment xml:lang="en">Priority level (1-4) based on Eisenhower Decision Matrix. 1=Urgent&amp;Important, 2=Important, 3=Urgent, 4=Neither.</rdfs:comment>
    </owl:DatatypeProperty>



    <!-- https://vocab.example.org/actions/v3#hasContext -->

    <owl:DatatypeProperty rdf:about="https://vocab.example.org/actions/v3#hasContext">
        <rdfs:domain rdf:resource="https://vocab.example.org/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has context</rdfs:label>
        <rdfs:comment xml:lang="en">GTD-style context tag indicating where, when, or with what tools this action can be performed (e.g., @phone, @computer, @office).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/location"/>
    </owl:DatatypeProperty>



    <!-- https://vocab.example.org/actions/v3#hasProject -->

    <owl:DatatypeProperty rdf:about="https://vocab.example.org/actions/v3#hasProject">
        <rdfs:domain rdf:resource="https://vocab.example.org/actions/v3#RootActionPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has project</rdfs:label>
        <rdfs:comment xml:lang="en">Project or story name that this root action plan belongs to. Only available for root action plans to maintain hierarchical clarity.</rdfs:comment>
    </owl:DatatypeProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Example Instances (POC)
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- Example 1: Simple Root Action -->

    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_review_reports_plan">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#RootActionPlan"/>
        <schema:name>Review quarterly reports</schema:name>
        <schema:description>Analyze Q4 2024 performance metrics and prepare summary</schema:description>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">2</hasPriority>
        <hasContext>@office</hasContext>
        <hasContext>@computer</hasContext>
        <prescribes rdf:resource="https://vocab.example.org/actions/v3#example_review_reports_process"/>
        <rdfs:comment>Simple root action plan example showing basic properties</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_review_reports_process">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionProcess"/>
        <hasState rdf:resource="https://vocab.example.org/actions/v3#Completed"/>
        <rdfs:comment>Execution of the review reports action - completed successfully</rdfs:comment>
    </owl:NamedIndividual>



    <!-- Example 2: Hierarchical Project -->

    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_launch_product_plan">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#RootActionPlan"/>
        <schema:name>Launch new product</schema:name>
        <schema:description>Complete product launch for Q1 2025</schema:description>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">1</hasPriority>
        <hasProject>Product Development</hasProject>
        <rdfs:comment>Root action plan with hierarchical decomposition</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_finalize_specs_plan">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ChildActionPlan"/>
        <schema:name>Finalize specifications</schema:name>
        <schema:description>Complete technical specifications document</schema:description>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">1</hasPriority>
        <hasContext>@computer</hasContext>
        <bfo:BFO_0000178 rdf:resource="https://vocab.example.org/actions/v3#example_launch_product_plan"/>
        <prescribes rdf:resource="https://vocab.example.org/actions/v3#example_finalize_specs_process"/>
        <rdfs:comment>Child action plan - part of product launch</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_finalize_specs_process">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionProcess"/>
        <hasState rdf:resource="https://vocab.example.org/actions/v3#Completed"/>
        <rdfs:comment>Completed execution of specifications task</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_update_docs_plan">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ChildActionPlan"/>
        <schema:name>Update documentation</schema:name>
        <schema:description>Update product documentation to reflect new specifications</schema:description>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">2</hasPriority>
        <hasContext>@computer</hasContext>
        <bfo:BFO_0000178 rdf:resource="https://vocab.example.org/actions/v3#example_launch_product_plan"/>
        <prescribes rdf:resource="https://vocab.example.org/actions/v3#example_update_docs_process"/>
        <rdfs:comment>Child action plan - depends on specs being finalized</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_update_docs_process">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#ActionProcess"/>
        <hasState rdf:resource="https://vocab.example.org/actions/v3#InProgress"/>
        <rdfs:comment>Currently executing documentation update</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.example.org/actions/v3#example_review_section_plan">
        <rdf:type rdf:resource="https://vocab.example.org/actions/v3#LeafActionPlan"/>
        <schema:name>Review API section</schema:name>
        <schema:description>Review and update the API documentation section</schema:description>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">2</hasPriority>
        <bfo:BFO_0000178 rdf:resource="https://vocab.example.org/actions/v3#example_update_docs_plan"/>
        <rdfs:comment>Leaf action plan - atomic task at maximum depth</rdfs:comment>
    </owl:NamedIndividual>


</rdf:RDF>
