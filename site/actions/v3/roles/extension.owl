        <skos:example>Plan date night, Help kids with homework, Call parents, Family vacation</skos:example>
    </owl:NamedIndividual>

    <!-- Home & Property -->
    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#HomeRole">
        <rdf:type rdf:resource="https://www.commoncoreontologies.org/ont00001366"/>
