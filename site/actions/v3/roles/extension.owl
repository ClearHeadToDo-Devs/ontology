    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Example Usage Patterns
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->

    <!-- Example 1: Personal GTD Action with Role -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#annual_checkup_plan">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <schema:name>Schedule annual physical exam</schema:name>
        <inRoleContext rdf:resource="https://vocab.clearhead.io/actions/roles#HealthRole"/>
        <assignedToAgent rdf:resource="https://vocab.clearhead.io/actions/roles#me"/>
        <rdfs:comment>Health-related action assigned to self</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#me">
        <rdf:type rdf:resource="https://www.commoncoreontologies.org/ont00001337"/>
        <rdfs:label>Me (current user)</rdfs:label>
        <rdfs:comment>The person using this productivity system</rdfs:comment>
    </owl:NamedIndividual>



    <!-- Example 2: Work Action with Role -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#learn_rust_plan">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <schema:name>Complete Rust programming course</schema:name>
        <inRoleContext rdf:resource="https://vocab.clearhead.io/actions/roles#CareerRole"/>
        <assignedToAgent rdf:resource="https://vocab.clearhead.io/actions/roles#me"/>
        <rdfs:comment>Professional development action</rdfs:comment>
    </owl:NamedIndividual>



    <!-- Example 3: Delegation Pattern -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#manager">
        <rdf:type rdf:resource="https://www.commoncoreontologies.org/ont00001337"/>
        <rdfs:label>Project Manager</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#junior_dev">
        <rdf:type rdf:resource="https://www.commoncoreontologies.org/ont00001337"/>
        <rdfs:label>Junior Developer</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#research_plan">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <schema:name>Research competitor features</schema:name>
        <delegatedBy rdf:resource="https://vocab.clearhead.io/actions/roles#manager"/>
        <assignedToAgent rdf:resource="https://vocab.clearhead.io/actions/roles#junior_dev"/>
        <rdfs:comment>Manager delegated to junior developer</rdfs:comment>
    </owl:NamedIndividual>



    <!-- Example 4: Team Collaboration -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#dev_team">
        <rdf:type rdf:resource="https://www.commoncoreontologies.org/ont00000593"/>
        <rdfs:label>Development Team</rdfs:label>
        <rdfs:comment>Group of agents (team)</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/roles#sprint_planning_plan">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <schema:name>Sprint planning meeting</schema:name>
        <assignedToAgent rdf:resource="https://vocab.clearhead.io/actions/roles#dev_team"/>
        <rdfs:comment>Assigned to entire team (group)</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
