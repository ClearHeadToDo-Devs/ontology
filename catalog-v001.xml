<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<catalog prefer="public" xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
    <!--
        XML Catalog for Actions Vocabulary v3 Extensions

        Maps deployed vocabulary URLs to local files for development.
        Protégé automatically reads this file when loading ontologies.

        This allows OWL files to contain production URIs while
        development works with local files.
    -->

    <group id="Folder Repository" prefer="public" xml:base="">
        <!-- BFO import -->
        <uri name="http://purl.obolibrary.org/obo/bfo.owl" uri="imports/bfo.owl"/>

        <!-- Core vocabulary - multiple URI patterns -->
        <uri name="https://vocab.clearhead.io/actions/v3/core" uri="actions-vocabulary.owl"/>
        <uri name="https://vocab.clearhead.io/actions/v3.1.0/core" uri="actions-vocabulary.owl"/>
        <uri name="https://vocab.clearhead.io/actions/v3" uri="actions-vocabulary.owl"/>
        <uri name="https://vocab.example.org/actions/v3" uri="actions-vocabulary.owl"/>

        <!-- Context extension -->
        <uri name="https://vocab.clearhead.io/actions/v3/context" uri="actions-context.owl"/>
        <uri name="https://vocab.clearhead.io/actions/v3.1.0/context" uri="actions-context.owl"/>
        <uri name="https://vocab.clearhead.io/actions/context" uri="actions-context.owl"/>

        <!-- Workflow extension -->
        <uri name="https://vocab.clearhead.io/actions/v3/workflow" uri="actions-workflow.owl"/>
        <uri name="https://vocab.clearhead.io/actions/v3.1.0/workflow" uri="actions-workflow.owl"/>
        <uri name="https://vocab.clearhead.io/actions/workflow" uri="actions-workflow.owl"/>

        <!-- Roles extension -->
        <uri name="https://vocab.clearhead.io/actions/v3/roles" uri="actions-roles.owl"/>
        <uri name="https://vocab.clearhead.io/actions/v3.1.0/roles" uri="actions-roles.owl"/>
        <uri name="https://vocab.clearhead.io/actions/roles" uri="actions-roles.owl"/>

        <!-- Agile extension (when created) -->
        <uri name="https://vocab.clearhead.io/actions/v3/agile" uri="actions-agile.owl"/>
        <uri name="https://vocab.clearhead.io/actions/v3.1.0/agile" uri="actions-agile.owl"/>
        <uri name="https://vocab.clearhead.io/actions/agile" uri="actions-agile.owl"/>
    </group>
</catalog>
