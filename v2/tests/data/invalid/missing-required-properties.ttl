@prefix actions: <https://vocab.example.org/actions/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

#################################################################
#    Invalid: Missing Required Properties
#################################################################

# Missing state
actions:missing_state a actions:RootAction ;
    actions:priority 1 ;
    actions:context "@work" .
    # Missing actions:state - Invalid!

# Missing priority
actions:missing_priority a actions:RootAction ;
    actions:state actions:NotStarted ;
    actions:context "@work" .
    # Missing actions:priority - Invalid!

# Missing any property - but this is actually valid since context is optional
# Let's make this a duplicate of missing_state for now
actions:duplicate_missing_state a actions:RootAction ;
    actions:priority 1 ;
    actions:context "@work" .
    # Missing actions:state - Invalid!