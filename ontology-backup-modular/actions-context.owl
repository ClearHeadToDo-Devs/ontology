    <!-- Common Location Contexts -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#HomeLocationContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#LocationContext"/>
        <rdfs:label xml:lang="en">Home Location Context</rdfs:label>
        <rdfs:comment>Reusable context for actions requiring being at home</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#OfficeLocationContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#LocationContext"/>
        <rdfs:label xml:lang="en">Office Location Context</rdfs:label>
        <rdfs:comment>Reusable context for actions requiring being at office</rdfs:comment>
    </owl:NamedIndividual>



    <!-- Common Tool Contexts -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#ComputerToolContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#ToolContext"/>
        <rdfs:label xml:lang="en">Computer Tool Context</rdfs:label>
        <rdfs:comment>Reusable context for actions requiring computer access</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#PhoneToolContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#ToolContext"/>
        <rdfs:label xml:lang="en">Phone Tool Context</rdfs:label>
        <rdfs:comment>Reusable context for actions requiring telephone</rdfs:comment>
    </owl:NamedIndividual>



    <!-- Common Energy Contexts -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#HighEnergyContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#EnergyContext"/>
        <hasEnergyLevel>high</hasEnergyLevel>
        <rdfs:label xml:lang="en">High Energy Context</rdfs:label>
        <rdfs:comment>For complex, demanding tasks requiring full cognitive capacity</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#MediumEnergyContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#EnergyContext"/>
        <hasEnergyLevel>medium</hasEnergyLevel>
        <rdfs:label xml:lang="en">Medium Energy Context</rdfs:label>
        <rdfs:comment>For routine work and standard tasks</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#LowEnergyContext">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/context#EnergyContext"/>
        <hasEnergyLevel>low</hasEnergyLevel>
        <rdfs:label xml:lang="en">Low Energy Context</rdfs:label>
        <rdfs:comment>For simple tasks doable when tired or distracted</rdfs:comment>
    </owl:NamedIndividual>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Example Usage
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->

    <!-- Example: Task requiring multiple contexts -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/context#example_write_report_plan">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <schema:name>Write quarterly report</schema:name>
        <schema:description>Compile Q4 metrics and analysis</schema:description>
        <requiresContext rdf:resource="https://vocab.clearhead.io/actions/context#OfficeLocationContext"/>
        <requiresContext rdf:resource="https://vocab.clearhead.io/actions/context#ComputerToolContext"/>
        <requiresContext rdf:resource="https://vocab.clearhead.io/actions/context#HighEnergyContext"/>
        <rdfs:comment>Demonstrates multiple contexts: needs office, computer, AND high energy</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
