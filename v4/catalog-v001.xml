<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<catalog prefer="public" xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
    <!--
        XML Catalog for Actions Vocabulary v4

        Maps CCO and BFO ontology IRIs to local files.
        Protégé reads this automatically when loading ontologies from this directory.

        Format follows the Protégé/CCO standard: flat <uri> entries with no <group xml:base>
        so that relative paths resolve correctly against this catalog file's location.

        CCO modules vendored from: https://github.com/CommonCoreOntology/CommonCoreOntologies
        CCO version: 2.0 (2024-11-06)
        BFO version: 2020 (bfo-core.ttl)
    -->

    <!-- BFO 2020 core (imported by ExtendedRelationOntology) -->
    <uri id="BFO 2020" name="http://purl.obolibrary.org/obo/bfo/2020/bfo-core.ttl" uri="imports/bfo-core.ttl"/>

    <!-- CCO 2.0 modules — all modules in our transitive import chain -->
    <uri id="ExtendedRelationOntology" name="https://www.commoncoreontologies.org/ExtendedRelationOntology" uri="imports/ExtendedRelationOntology.ttl"/>
    <uri id="GeospatialOntology" name="https://www.commoncoreontologies.org/GeospatialOntology" uri="imports/GeospatialOntology.ttl"/>
    <uri id="TimeOntology" name="https://www.commoncoreontologies.org/TimeOntology" uri="imports/TimeOntology.ttl"/>
    <uri id="InformationEntityOntology" name="https://www.commoncoreontologies.org/InformationEntityOntology" uri="imports/InformationEntityOntology.ttl"/>
    <uri id="EventOntology" name="https://www.commoncoreontologies.org/EventOntology" uri="imports/EventOntology.ttl"/>
    <uri id="AgentOntology" name="https://www.commoncoreontologies.org/AgentOntology" uri="imports/AgentOntology.ttl"/>
</catalog>
