@prefix actions: <https://vocab.example.org/actions/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

#################################################################
#    Valid Test Cases - These should pass validation
#################################################################

# Valid Root Action (depth 0) with project
actions:root1 a actions:RootAction ;
    actions:depth 0 ;
    actions:priority 1 ;
    actions:project "Website Redesign" ;
    actions:state actions:InProgress ;
    actions:context "@computer", "@office" ;
    actions:childAction actions:child1 .

# Valid Child Action (depth 1) 
actions:child1 a actions:ChildAction ;
    actions:depth 1 ;
    actions:priority 2 ;
    actions:state actions:NotStarted ;
    actions:context "@design" ;
    actions:parentAction actions:root1 ;
    actions:childAction actions:child2, actions:leaf1 ;
    actions:doDate "2025-01-25"^^xsd:date .

# Valid Child Action (depth 2)
actions:child2 a actions:ChildAction ;
    actions:depth 2 ;
    actions:priority 3 ;
    actions:state actions:InProgress ;
    actions:context "@computer" ;
    actions:parentAction actions:child1 ;
    actions:childAction actions:child3 ;
    actions:durationMinutes 90 .

# Valid Child Action (depth 3)
actions:child3 a actions:ChildAction ;
    actions:depth 3 ;
    actions:priority 2 ;
    actions:state actions:Blocked ;
    actions:context "@phone" ;
    actions:parentAction actions:child2 ;
    actions:childAction actions:child4 .

# Valid Child Action (depth 4) 
actions:child4 a actions:ChildAction ;
    actions:depth 4 ;
    actions:priority 4 ;
    actions:state actions:NotStarted ;
    actions:context "@errands" ;
    actions:parentAction actions:child3 ;
    actions:childAction actions:leaf2 .

# Valid Leaf Action (depth 5)
actions:leaf1 a actions:LeafAction ;
    actions:depth 5 ;
    actions:priority 1 ;
    actions:state actions:Completed ;
    actions:context "@computer" ;
    actions:parentAction actions:child1 ;
    actions:completedDateTime "2025-01-20T14:30:00"^^xsd:dateTime .

# Valid Leaf Action (depth 5) 
actions:leaf2 a actions:LeafAction ;
    actions:depth 5 ;
    actions:priority 3 ;
    actions:state actions:NotStarted ;
    actions:context "@home", "@weekend" ;
    actions:parentAction actions:child4 .

# Valid Root Action with recurrence
actions:root2 a actions:RootAction ;
    actions:depth 0 ;
    actions:priority 2 ;
    actions:project "Daily Standup" ;
    actions:state actions:InProgress ;
    actions:context "@meeting" ;
    actions:recurrenceFrequency "DAILY" ;
    actions:recurrenceInterval 1 ;
    actions:recurrenceUntil "2025-12-31"^^xsd:date ;
    actions:durationMinutes 15 .

# Valid standalone Root Action (no project)
actions:root3 a actions:RootAction ;
    actions:depth 0 ;
    actions:priority 1 ;
    actions:state actions:NotStarted ;
    actions:context "@personal" ;
    actions:doDateTime "2025-01-25T09:00:00"^^xsd:dateTime .
