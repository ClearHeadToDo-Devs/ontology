@prefix actions: <https://vocab.example.org/actions/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

#################################################################
#    Invalid: Both Recurrence Until and Count
#################################################################

actions:recurrence_invalid a actions:RootAction ;
    actions:priority 1 ;
    actions:state actions:NotStarted ;
    actions:context "@work" ;
    actions:recurrenceFrequency "WEEKLY" ;
    actions:recurrenceUntil "2025-12-31"^^xsd:date ;
    actions:recurrenceCount 52 .  # Invalid! Can't have both until and count