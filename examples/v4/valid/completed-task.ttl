@prefix actions: <https://clearhead.us/vocab/actions/v4#> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================================
# Completed Task Example
# =============================================================================
#
# Demonstrates:
# 1. A completed PlannedAct (phase = Completed)
# 2. Temporal properties on the Plan (scheduled time)
# 3. Completion time tracking on the PlannedAct
#
# DSL equivalent: [x] Call dentist @2025-01-15T10:00 %2025-01-15T10:15
# =============================================================================

# The Plan with scheduling information
<urn:uuid:plan-call-dentist> a cco:ont00000974 ;
    rdfs:label "Call dentist" ;
    rdfs:comment "Schedule dental cleaning appointment" ;
    actions:hasDoDateTime "2025-01-15T10:00:00Z"^^xsd:dateTime ;
    cco:ont00001942 <urn:uuid:act-call-dentist-001> .

# The completed PlannedAct
<urn:uuid:act-call-dentist-001> a cco:ont00000228 ;
    actions:hasPhase actions:Completed ;
    actions:hasCompletedDateTime "2025-01-15T10:15:00Z"^^xsd:dateTime .