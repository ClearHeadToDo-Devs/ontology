@prefix actions: <https://clearhead.us/vocab/actions/v4#> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =============================================================================
# Invalid Example: Missing Completion Date
# =============================================================================
#
# This example should FAIL SHACL validation:
# PlannedAct has status=Completed but no hasCompletedDateTime
#
# SHACL rule: actions:CompletedActDateShape should catch this
# =============================================================================

<urn:plan:completed-without-date> a cco:ont00000974 ;
    rdfs:label "Completed task without date" ;
    cco:ont00001942 <urn:act:invalid> .

<urn:act:invalid> a cco:ont00000228 ;
    cco:ont00001868 actions:Completed .
    # Missing: actions:hasCompletedDateTime
