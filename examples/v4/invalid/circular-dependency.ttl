@prefix actions: <https://clearhead.us/vocab/actions/v4#> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =============================================================================
# Invalid Example: Circular Dependency
# =============================================================================
#
# This example should FAIL SHACL validation due to circular dependencies:
# Plan A is_successor_of Plan B, and Plan B is_successor_of Plan A
#
# SHACL rule: actions:NoCyclicDependenciesShape should catch this
# =============================================================================

<urn:plan:a> a cco:ont00000974 ;
    rdfs:label "Plan A" ;
    cco:ont00001775 <urn:plan:b> ;  # A is_successor_of B
    cco:ont00001942 <urn:act:a> .

<urn:act:a> a cco:ont00000228 ;
    cco:ont00001868 actions:NotStarted .

<urn:plan:b> a cco:ont00000974 ;
    rdfs:label "Plan B" ;
    cco:ont00001775 <urn:plan:a> ;  # B is_successor_of A (CIRCULAR!)
    cco:ont00001942 <urn:act:b> .

<urn:act:b> a cco:ont00000228 ;
    cco:ont00001868 actions:NotStarted .
