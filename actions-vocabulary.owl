<?xml version="1.0"?>
<rdf:RDF xmlns="https://vocab.clearhead.io/actions/v3#"
     xml:base="https://vocab.clearhead.io/actions/v3"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:bfo="http://purl.obolibrary.org/obo/BFO_"
     xmlns:cco="https://www.commoncoreontologies.org/"
     xmlns:schema="http://schema.org/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:dc="http://purl.org/dc/terms/">

    <owl:Ontology rdf:about="https://vocab.clearhead.io/actions/v3">
        <owl:versionIRI rdf:resource="https://vocab.clearhead.io/actions/v3/3.1.0"/>
        <dc:creator>Darrion Burgess</dc:creator>
        <dc:contributor>Claude (AI Assistant)</dc:contributor>
        <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-25</dc:created>
        <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-29</dc:modified>
        <dc:title>Actions Vocabulary v3 - Complete</dc:title>
        <rdfs:label>Actions Vocabulary (BFO/CCO-aligned) - Production</rdfs:label>
        <rdfs:comment>BFO and CCO-aligned ontology for action planning and execution. This consolidated ontology includes the core vocabulary plus context, workflow, and role integration extensions.</rdfs:comment>
        <owl:versionInfo>3.1.0</owl:versionInfo>

        <!-- Import BFO and CCO -->
        <!-- NOTE: Imports commented out for Python testing. Uncomment for Protégé. -->
        <!-- <owl:imports rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/InformationEntityOntology"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ArtifactOntology"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/FacilityOntology"/> -->
        <!-- <owl:imports rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Mid/AgentOntology"/> -->
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // ANNOTATIONS FOR IMPORTED CLASSES
    // (Provides human-readable labels for BFO/CCO classes we extend)
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- BFO Classes -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000003">
        <rdfs:label xml:lang="en">Occurrent</rdfs:label>
        <rdfs:comment xml:lang="en">BFO: An entity that unfolds over time and has temporal parts. Processes, events, and temporal regions are occurrents.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000019">
        <rdfs:label xml:lang="en">Quality</rdfs:label>
        <rdfs:comment xml:lang="en">BFO: A specifically dependent continuant that inheres in a bearer. Examples: temperature, color, state.</rdfs:comment>
    </owl:Class>

    <!-- CCO Classes -->

    <owl:Class rdf:about="https://www.commoncoreontologies.org/ont00000001">
        <rdfs:label xml:lang="en">Artifact</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: A material entity that has been designed to have a specific function. Examples: computer, telephone, tool.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="https://www.commoncoreontologies.org/ont00000192">
        <rdfs:label xml:lang="en">Facility</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: A material entity that is designed to support some process or activity. Examples: office, home, retail store.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="https://www.commoncoreontologies.org/ont00000228">
        <rdfs:label xml:lang="en">Planned Act</rdfs:label>
        <skos:altLabel xml:lang="en">Intentional Act</skos:altLabel>
        <rdfs:comment xml:lang="en">CCO: An act in which at least one agent plays a causative role and which is prescribed by some directive information content entity.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="https://www.commoncoreontologies.org/ont00000374">
        <rdfs:label xml:lang="en">Agent</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: A material entity that has the capability to perform actions. Includes persons, organizations, and groups.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="https://www.commoncoreontologies.org/ont00000965">
        <rdfs:label xml:lang="en">Directive Information Content Entity</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: An information content entity that consists of propositions or images that prescribe some entity. Examples: plans, specifications, algorithms.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="https://www.commoncoreontologies.org/ont00001366">
        <rdfs:label xml:lang="en">Role</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: A realizable entity that exists because there is some corresponding social norm or expectation. Examples: occupation, organization membership, social position.</rdfs:comment>
    </owl:Class>

    <!-- CCO Object Properties -->

    <owl:ObjectProperty rdf:about="https://www.commoncoreontologies.org/ont00001449">
        <rdfs:label xml:lang="en">has agent</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: Relates an act to an agent that participates in or performs it.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="https://www.commoncoreontologies.org/ont00001942">
        <rdfs:label xml:lang="en">prescribes</rdfs:label>
        <rdfs:comment xml:lang="en">CCO: Relates a directive information content entity to the entity it prescribes. If the prescribed entity is an occurrent, the ICE serves as a rule/guide; if continuant, it serves as a model.</rdfs:comment>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // CORE ACTION CLASSES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#ActionPlan -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#ActionPlan">
        <rdfs:subClassOf rdf:resource="https://www.commoncoreontologies.org/ont00000965"/>
        <skos:closeMatch rdf:resource="http://schema.org/Action"/>
        <rdfs:label xml:lang="en">Action Plan</rdfs:label>
        <skos:definition xml:lang="en">A directive information content entity that prescribes an action to be performed.</skos:definition>
        <rdfs:comment xml:lang="en">ActionPlans are BFO continuants (generically dependent continuants) that exist across time and can prescribe multiple action processes. An action plan can be executed multiple times (e.g., recurring actions). Closely aligned with Schema.org Action semantics via SKOS mapping.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#ActionProcess -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#ActionProcess">
        <rdfs:subClassOf rdf:resource="https://www.commoncoreontologies.org/ont00000228"/>
        <rdfs:label xml:lang="en">Action Process</rdfs:label>
        <skos:definition xml:lang="en">A planned act that is the actual execution of an action plan.</skos:definition>
        <rdfs:comment xml:lang="en">ActionProcesses are BFO occurrents (processes) that unfold over time and have temporal parts. They are prescribed by ActionPlans and represent the concrete realization of the plan's directives. Each execution of an action plan creates a distinct action process.</rdfs:comment>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // HIERARCHICAL ACTION PLAN CLASSES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#RootActionPlan -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#RootActionPlan">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <owl:disjointWith rdf:resource="https://vocab.clearhead.io/actions/v3#ChildActionPlan"/>
        <owl:disjointWith rdf:resource="https://vocab.clearhead.io/actions/v3#LeafActionPlan"/>
        <rdfs:label xml:lang="en">Root Action Plan</rdfs:label>
        <rdfs:comment xml:lang="en">A top-level action plan at hierarchical depth 0. Root action plans can belong to projects and serve as parents to child action plans. They cannot have parent actions.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#ChildActionPlan -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#ChildActionPlan">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <owl:disjointWith rdf:resource="https://vocab.clearhead.io/actions/v3#LeafActionPlan"/>
        <rdfs:label xml:lang="en">Child Action Plan</rdfs:label>
        <rdfs:comment xml:lang="en">An intermediate action plan at hierarchical depths 1-4. Child action plans must have a parent action and may have child or leaf action plans as parts.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#LeafActionPlan -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#LeafActionPlan">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:label xml:lang="en">Leaf Action Plan</rdfs:label>
        <rdfs:comment xml:lang="en">A terminal action plan at hierarchical depth 5 (maximum). Leaf action plans must have a parent child action and cannot have children of their own.</rdfs:comment>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // ACTION STATE ENUMERATION
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#ActionState -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#ActionState">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:label xml:lang="en">Action State</rdfs:label>
        <rdfs:comment xml:lang="en">An enumeration of states that an action process can be in. These are BFO qualities that inhere in action processes.</rdfs:comment>
    </owl:Class>



    <!-- State Individuals -->

    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/v3#NotStarted">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Not Started</rdfs:label>
        <rdfs:comment xml:lang="en">Action process has not been initiated.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/v3#InProgress">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">In Progress</rdfs:label>
        <rdfs:comment xml:lang="en">Action process is currently being executed.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/v3#Completed">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Completed</rdfs:label>
        <rdfs:comment xml:lang="en">Action process has been successfully completed.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/v3#Blocked">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Blocked</rdfs:label>
        <rdfs:comment xml:lang="en">Action process is blocked waiting for external dependency.</rdfs:comment>
    </owl:NamedIndividual>



    <owl:NamedIndividual rdf:about="https://vocab.clearhead.io/actions/v3#Cancelled">
        <rdf:type rdf:resource="https://vocab.clearhead.io/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">Cancelled</rdfs:label>
        <rdfs:comment xml:lang="en">Action process has been cancelled and will not be completed.</rdfs:comment>
    </owl:NamedIndividual>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // CONTEXT EXTENSION CLASSES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#ActionContext -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#ActionContext">
        <rdfs:subClassOf rdf:resource="https://www.commoncoreontologies.org/ont00000965"/>
        <rdfs:label xml:lang="en">Action Context</rdfs:label>
        <skos:definition xml:lang="en">A directive information content entity that prescribes environmental, resource, or situational requirements for performing an action.</skos:definition>
        <rdfs:comment xml:lang="en">Contexts are continuants (information entities) that persist and can be reused across multiple action plans. They prescribe what resources or conditions are needed for execution. GTD pattern: Replaces string-based @context tags with typed entities.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#LocationContext -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#LocationContext">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionContext"/>
        <rdfs:label xml:lang="en">Location Context</rdfs:label>
        <skos:definition xml:lang="en">A context specifying that an action requires being performed at a particular type of facility or geospatial location.</skos:definition>
        <rdfs:comment xml:lang="en">Links to CCO Facility hierarchy (47+ facility types). GTD use case: @office, @home, @errands contexts.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#ToolContext -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#ToolContext">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionContext"/>
        <rdfs:label xml:lang="en">Tool Context</rdfs:label>
        <skos:definition xml:lang="en">A context specifying that an action requires the use of particular artifacts or tools.</skos:definition>
        <rdfs:comment xml:lang="en">Links to CCO Artifact hierarchy (200+ artifact types). GTD use case: @computer, @phone, @online contexts.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#EnergyContext -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#EnergyContext">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionContext"/>
        <rdfs:label xml:lang="en">Energy Context</rdfs:label>
        <skos:definition xml:lang="en">A context specifying the cognitive or physical energy level required to perform an action.</skos:definition>
        <rdfs:comment xml:lang="en">Based on GTD principle that energy is a limiting resource alongside time and tools. Levels: high, medium, low.</rdfs:comment>
    </owl:Class>



    <!-- https://vocab.clearhead.io/actions/v3#SocialContext -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#SocialContext">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#ActionContext"/>
        <rdfs:label xml:lang="en">Social Context</rdfs:label>
        <skos:definition xml:lang="en">A context specifying that an action requires the presence or participation of particular agents.</skos:definition>
        <rdfs:comment xml:lang="en">Links to CCO Agent (Person, Organization, Group). GTD use case: @agenda contexts (discuss with specific person).</rdfs:comment>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // WORKFLOW EXTENSION CLASSES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#Milestone -->

    <owl:Class rdf:about="https://vocab.clearhead.io/actions/v3#Milestone">
        <rdfs:subClassOf rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <rdfs:label xml:lang="en">Milestone</rdfs:label>
        <skos:definition xml:lang="en">A significant checkpoint or goal in a project, often used as a dependency target for multiple actions.</skos:definition>
        <rdfs:comment xml:lang="en">Milestones represent completion of a set of actions and mark significant progress points. Typically don't prescribe processes themselves.</rdfs:comment>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // OBJECT PROPERTIES - CORE
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#prescribes -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#prescribes">
        <rdfs:subPropertyOf rdf:resource="https://www.commoncoreontologies.org/ont00001942"/>
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionProcess"/>
        <rdfs:label xml:lang="en">prescribes</rdfs:label>
        <skos:definition xml:lang="en">Relates an action plan to the action process it prescribes.</skos:definition>
        <rdfs:comment xml:lang="en">Specialized version of CCO's prescribes property (ont00001942) with domain and range restrictions. An action plan can prescribe multiple processes (e.g., recurring actions).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#hasState -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#hasState">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionProcess"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionState"/>
        <rdfs:label xml:lang="en">has state</rdfs:label>
        <rdfs:comment xml:lang="en">Relates an action process to its current state. Functional property - each process has exactly one state.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/actionStatus"/>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // OBJECT PROPERTIES - CONTEXT EXTENSION
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#requiresContext -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#requiresContext">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionContext"/>
        <rdfs:label xml:lang="en">requires context</rdfs:label>
        <skos:definition xml:lang="en">Links an action plan to the contexts required for its execution.</skos:definition>
        <rdfs:comment xml:lang="en">Replaces v2's string-based hasContext property. Multiple contexts can be specified (AND relationship).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#requiresFacility -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#requiresFacility">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#LocationContext"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00000192"/>
        <rdfs:label xml:lang="en">requires facility</rdfs:label>
        <skos:definition xml:lang="en">Specifies the type of facility needed for action execution.</skos:definition>
        <rdfs:comment xml:lang="en">Links to cco:Facility (ont00000192). Can specify facility TYPE or specific INSTANCE.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#requiresArtifact -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#requiresArtifact">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ToolContext"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00000001"/>
        <rdfs:label xml:lang="en">requires artifact</rdfs:label>
        <skos:definition xml:lang="en">Specifies artifacts or tools needed for action execution.</skos:definition>
        <rdfs:comment xml:lang="en">Links to cco:Artifact (ont00000001). Can specify artifact TYPE or specific INSTANCE.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#requiresAgent -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#requiresAgent">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#SocialContext"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00000374"/>
        <rdfs:label xml:lang="en">requires agent</rdfs:label>
        <skos:definition xml:lang="en">Specifies other agents whose presence or participation is needed.</skos:definition>
        <rdfs:comment xml:lang="en">Links to cco:Agent (ont00000374). Multiple agents can be required.</rdfs:comment>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // OBJECT PROPERTIES - ROLE EXTENSION
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#assignedToAgent -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#assignedToAgent">
        <rdfs:subPropertyOf rdf:resource="https://www.commoncoreontologies.org/ont00001449"/>
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00000374"/>
        <rdfs:label xml:lang="en">assigned to agent</rdfs:label>
        <skos:definition xml:lang="en">Specifies which agent is responsible for executing this action plan.</skos:definition>
        <rdfs:comment xml:lang="en">Prescriptive agent assignment at plan level. WHO SHOULD perform this action.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#performedBy -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#performedBy">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionProcess"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00000374"/>
        <rdfs:label xml:lang="en">performed by</rdfs:label>
        <skos:definition xml:lang="en">Specifies which agent actually executed this action process.</skos:definition>
        <rdfs:comment xml:lang="en">Descriptive agent tracking at process level. WHO ACTUALLY performed this action.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#delegatedBy -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#delegatedBy">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00000374"/>
        <rdfs:label xml:lang="en">delegated by</rdfs:label>
        <skos:definition xml:lang="en">Indicates the original responsible party who delegated this action to another agent.</skos:definition>
        <rdfs:comment xml:lang="en">Tracks delegation chain. Original owner still accountable even though delegated.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#inRoleContext -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#inRoleContext">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://www.commoncoreontologies.org/ont00001366"/>
        <rdfs:label xml:lang="en">in role context</rdfs:label>
        <skos:definition xml:lang="en">Indicates which role or area of responsibility this action belongs to.</skos:definition>
        <rdfs:comment xml:lang="en">GTD Areas of Focus pattern using CCO Roles. Enables work-life balance tracking and weekly review by area.</rdfs:comment>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // OBJECT PROPERTIES - WORKFLOW EXTENSION
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://vocab.clearhead.io/actions/v3#dependsOn -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#dependsOn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:label xml:lang="en">depends on</rdfs:label>
        <skos:definition xml:lang="en">Indicates that this action plan should not begin execution until the target action plan has been completed.</skos:definition>
        <rdfs:comment xml:lang="en">General dependency relation for plans (continuants). Transitive property. Prescriptive information (what SHOULD happen).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#cannotStartUntil -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#cannotStartUntil">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <owl:inverseOf rdf:resource="https://vocab.clearhead.io/actions/v3#mustCompleteBefore"/>
        <rdfs:label xml:lang="en">cannot start until</rdfs:label>
        <skos:definition xml:lang="en">Hard constraint: this action plan cannot begin until the target plan completes.</skos:definition>
        <rdfs:comment xml:lang="en">Stronger than dependsOn - CANNOT vs SHOULD NOT. Mandatory constraint.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#mustCompleteBefore -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#mustCompleteBefore">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <owl:inverseOf rdf:resource="https://vocab.clearhead.io/actions/v3#cannotStartUntil"/>
        <rdfs:label xml:lang="en">must complete before</rdfs:label>
        <skos:definition xml:lang="en">Hard constraint: this action must finish before the target action can start.</skos:definition>
        <rdfs:comment xml:lang="en">Inverse of cannotStartUntil (same relationship, different direction).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#preferredAfter -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#preferredAfter">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:label xml:lang="en">preferred after</rdfs:label>
        <skos:definition xml:lang="en">Soft constraint: this action is preferably done after the target, but not required.</skos:definition>
        <rdfs:comment xml:lang="en">Weakest constraint - optimization hint, not requirement. Recommendation, not mandate.</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#blockedBy -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#blockedBy">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionProcess"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000003"/>
        <rdfs:label xml:lang="en">blocked by</rdfs:label>
        <skos:definition xml:lang="en">Indicates that this action process cannot continue until the specified occurrent completes.</skos:definition>
        <rdfs:comment xml:lang="en">Runtime blocking relationship between processes (occurrents). Descriptive (what IS happening). Can be blocked by external event (delivery, weather, approval process).</rdfs:comment>
    </owl:ObjectProperty>



    <!-- https://vocab.clearhead.io/actions/v3#canRunInParallel -->

    <owl:ObjectProperty rdf:about="https://vocab.clearhead.io/actions/v3#canRunInParallel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:label xml:lang="en">can run in parallel</rdfs:label>
        <skos:definition xml:lang="en">Indicates these actions have no ordering constraints and can be executed simultaneously.</skos:definition>
        <rdfs:comment xml:lang="en">Explicit declaration that actions are independent. Symmetric property. Optimization hint for parallel execution.</rdfs:comment>
    </owl:ObjectProperty>



