     -->

    <!-- https://vocab.clearhead.io/actions/v3#hasPriority -->

    <owl:DatatypeProperty rdf:about="https://vocab.clearhead.io/actions/v3#hasPriority">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <rdfs:label xml:lang="en">has priority</rdfs:label>
        <rdfs:comment xml:lang="en">Priority level (1-4) based on Eisenhower Decision Matrix. 1=Urgent&amp;Important, 2=Important, 3=Urgent, 4=Neither.</rdfs:comment>
    </owl:DatatypeProperty>



    <!-- https://vocab.clearhead.io/actions/v3#hasContext (deprecated - use requiresContext) -->

    <owl:DatatypeProperty rdf:about="https://vocab.clearhead.io/actions/v3#hasContext">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#ActionPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has context</rdfs:label>
        <rdfs:comment xml:lang="en">DEPRECATED: Use requiresContext instead. GTD-style context tag indicating where, when, or with what tools this action can be performed (e.g., @phone, @computer, @office).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/location"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:DatatypeProperty>



    <!-- https://vocab.clearhead.io/actions/v3#hasProject -->

    <owl:DatatypeProperty rdf:about="https://vocab.clearhead.io/actions/v3#hasProject">
        <rdfs:domain rdf:resource="https://vocab.clearhead.io/actions/v3#RootActionPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has project</rdfs:label>
        <rdfs:comment xml:lang="en">Project or story name that this root action plan belongs to. Only available for root action plans to maintain hierarchical clarity.</rdfs:comment>
    </owl:DatatypeProperty>



